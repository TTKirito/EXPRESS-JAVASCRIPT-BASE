{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-undef": "off",
      "no-duplicate-case": "error",
      "no-use-before-define": "error",
      "no-unused-vars": "warn",
      "no-useless-catch": "warn",
      "no-empty-pattern": "warn",
      "no-empty": "warn"
    }
}
